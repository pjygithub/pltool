                        cellProperties.renderer = colorRowRenderer_0;
                        break;
                }
                return cellProperties;
            },
            licenseKey: 'non-commercial-and-evaluation', // for non-commercial use only
            //扩展最后一列，其他列的宽度是47
            stretchH: 'last',
            //把table的宽度设为容器的宽度，列平分宽度
            stretchH: 'all',
            //默认值
            stretchH: 'none',
                    });
        var date = new Date();
        var hour = date.getHours(); // 获取当前小时数(0-23)
        var min = date.getMinutes(); // 获取当前分钟数(0-59)
        var sec = date.getSeconds(); // 获取当前秒数(0-59)
        if (hour < 10) hour = "0" + hour;
        if (min < 10) min = "0" + min;
        if (sec < 10) sec = "0" + sec;
        const exportPlugin = hot.getPlugin('exportFile');
         button.addEventListener('click', () => {
            exportPlugin.downloadFile('csv', {
                bom: true,
                columnDelimiter: ',',
                columnHeaders: colHeader,
                exportHiddenColumns: true,
                exportHiddenRows: true,
                fileExtension: 'csv',
                filename: 'mID_#testRSA - func_downtimelog - line_line0 - model_normal@[YYYY]-[MM]-[DD]__' + hour + "_" + min + "_" + sec + "_" + getRnd(1000, 9999) + '筛选导出的_©powerByHandsontable©',
                mimeType: 'text/csv',
                rowDelimiter: '\r\n',
                rowHeaders: false,
            });
        });
    </script>
    <style>
        td {
            font-size: 13px;
        }
    </style>
</body>


